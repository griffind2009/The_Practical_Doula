<h1> Create a New Birth Form </h1>

<%= form_for [@client, @birth] do |f| %>
  <%= f.label :medication %>
  <%= f.text_field :medication %>
  <%= f.label :intervention %>
  <%= f.text_field :intervention %>
  <%= f.label :dilation %>
  <%= f.text_field :dilation %>
  <%= f.label :effacement %>
  <%= f.text_field :effacement %>
  <%= f.submit "Create"%>
<% end %>
